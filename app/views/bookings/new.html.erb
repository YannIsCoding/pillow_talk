<h1 class='title-form'>This is the booking page for the pillows!</h1>
<div class="form-container">
   <%= simple_form_for([@pillow, @booking]) do |f| %>
    <%= f.error_notification %>
    <div class=“form-inputs”>
      <%= f.input :start_date %>
      <%= f.input :end_date %>
    </div>
    <div class=“form-actions”>
      <%= f.button :submit, class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
